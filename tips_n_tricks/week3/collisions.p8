								64, 102, {13,t%80>40 and 12 or 1,13}, true)
	printc('the "player" box', 64, 108, 13)
end

--- helpers ---

-- can't concat those other things..
function str(i)
 local t=type(i)
 if(t=="boolean")return i and"true"or"false"
 if(t=="table"or t=="function"or t=="nil")return t
 return i..''
end

-- for print fancy :p
-- prints strings concatenated
-- but in the colors specified
-- example usage:
--  printf({'hello ','world'},
--		 						64,64,
--		 						{8,11})

function printf(words,x,y,cs,center)
 local cursor = 0
 if center then
 	for i, w in pairs(words) do 
 		x -= #str(w)*2
 	end
 	y -= 2.5
 end
	for i, w in pairs(words) do 
		print(w, x + cursor*4, y, cs[i])
		cursor += #str(w)
	end
end

function printc(s,x,y,c)
	if(c)return print(s,x-2*#str(s),y,c)
	print(s,x-2*#str(s),y,c)
end

-- draws a dotted line
-- w is width of dots

function dotline(x1,y1,x2,y2,w,c)
 w = w or 1
 w -= 1
	local d = distance(x1,y1,x2,y2)
	local dx, dy = x2-x1, y2-y1
	local sx = d==0 and 0 or dx/d
	local sy = d==0 and 0 or dy/d
	if c then 
		for i=0,d,(w+1)*2 do
			line(x1 + sx*i, y1 + sy*i,
								x1 + sx*(i+w), y1 + sy*(i+w),
								c)
		end
	else 
		for i=0,d,2 do
			line(x1 + sx*i, y1 + sy*i,
								x1 + sx*(i+w), y1 + sy*(i+w))
		end
	end
end

-- approximate distance

function distance(x1,y1,x2,y2)
	local dx = abs(x2-x1)
	local dy = abs(y2-y1)
	return dx+dy-0.585786*min(dx,dy)
end

