 ------------------------------
 ------- distance-width -------
 ------------------------------

function init_dw()
 box={x=44,y=50-25,w=50}
 p.x=8
 p.y=50-15
 p.w=30
 colliding = false
end

function hit_dw()
	-- for demonstration
	-- restrict movement to x
	p.y=50-15

	-- box center
	bx = box.x + box.w/2
	by = box.y + box.w/2
	-- player center
	px = p.x + p.w/2
	py = p.y + p.w/2

 -- abs(bx-py) is the distance 
 -- between the 2 centers
 inx=abs(bx-px) < box.w/2+p.w/2
	iny=abs(by-py) < box.w/2+p.w/2

	--if distance between the
	--centers is < half their widths
	--they are colliding
 if inx and iny then 
  colliding = true
 else 
  colliding = false
 end
end

function draw_dw()
	cls()

	phw = p.w/2
	bhw = box.w/2

	-- boxes
 c = colliding and 14 or 5

	rect(box.x,box.y,
						box.x+box.w,box.y+box.w,
						5)

	rect(p.x,p.y,
						p.x+p.w,p.y+p.w,
						c)
	if colliding then 
		rectfill(box.x,box.y,
							box.x+box.w,box.y+box.w,
							15)

		rectfill(p.x,p.y,
							p.x+p.w,p.y+p.w,
							c)
	end


	-- distance line
	xc = inx and 8 or 6
	dotline(bx+1,by+1, px+1,by+1, 2,0)
	dotline(bx,by, px,by, 2,xc)

 -- centers
	circfill(bx+1,by+1, 1,0)
	circfill(px+1,py+1, 1,0)

	circfill(bx,by, 1,11)
	circfill(px,py, 1,10)

	-- player half-width
	line(px,      box.y+box.w+4,
						p.x+p.w, box.y+box.w+4,4)
	pset(px,      box.y+box.w+3,4)
	pset(p.x+p.w, box.y+box.w+3,4)

	--[[printc(phw, px+phw/2 + .5, 
								box.y+box.w+6,1)--]]

	-- box half-width
	line(bx,      box.y+box.w+4,
						box.x,   box.y+box.w+4,3)
	pset(bx,      box.y+box.w+3,3)
	pset(box.x,   box.y+box.w+3,3)

	--[[printc(bhw, bx-bhw/2 + .5, 
								box.y+box.w+6,3)--]]


	-- dynamic info (distance)
--[[	printf({'abs(',px,'-',bx,'): ',
									abs(px-bx),' < ',
									phw,'+',bhw},
								64,2.5,
								{6,10,6,11,6,
									6,xc,
									1,6,3},true)--]]

	s = {'abs(','x1','-','x2',') < ',
					 'w1/2',' + ','w2/2',': ',colliding}
	printf(s,3,105,
							{6,10,6,11,6,
								4,6,3,6,xc})

	printf({'box centers,',' not top-left'},
								7,119, {2,2})
	line(29-7, 112,
						29-7, 116,2)
	line(29+5, 112,
						29+5, 116,2)
end

