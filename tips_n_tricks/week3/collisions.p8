function init_map()
 p={x=62,y=102}
 zoom = 3.055
 no_draw = 1.25
 -- let's use acceleration
 -- it should make the collisions
 -- look better
 dx = 0
 dy = 0
end

function hit_map()
 if (btn(4)) zoom = max(1,zoom-zoom*.01)
 if (btn(5)) zoom = min(100,zoom+zoom*.01)

 -- don't want to move yet..
 -- reverse all the buttons!
 if(btn(0))p.x += 2
 if(btn(1))p.x -= 2
 if(btn(2))p.y += 2
 if(btn(3))p.y -= 2


 if(btn(0))dx -= .2
 if(btn(1))dx += .2
 if(btnp(2))dy -= 2
 if(btn(3))dy += .2

 nx = p.x+dx
 ny = p.y+dy

 collisions = {false,false,false,false}

 if fget(mget((nx+(sgn(dx)+1)*4)/8, p.y/8),   0) or
 			fget(mget((nx+(sgn(dx)+1)*4)/8, (p.y+8)/8), 0) then
 	if fget(mget((nx+(sgn(dx)+1)*4)/8, p.y/8),   0) then
 		if dx < 0 then collisions[1] = true
 												else collisions[2] = true end 
 	end
 	if fget(mget((nx+(sgn(dx)+1)*4)/8, (p.y+8)/8), 0) then
 		if dx < 0 then collisions[3] = true
 												else collisions[4] = true end
 	end
 	dx = -dx*.4
 else
  p.x = nx
 end

 if fget(mget(p.x/8,   (ny+(sgn(dy)+1)*4)/8), 0) or
 			fget(mget((p.x+8)/8, (ny+(sgn(dy)+1)*4)/8), 0) then
 	if fget(mget(p.x/8,   (ny+(sgn(dy)+1)*4)/8), 0) then
 		if dy < 0 then collisions[1] = true
 												else collisions[3] = true end
 	end
 	if fget(mget((p.x+8)/8, (ny+(sgn(dy)+1)*4)/8), 0) then
 		if dy < 0 then collisions[2] = true
 												else collisions[4] = true end
 	end
 	dy = -dy*.4
 else
  p.y = ny
 end

 -- gravity
 dy += .1

 -- friction
 dx *= .8

end

function draw_map()
	cls()

	-- move cam
	-- keep it within bounds
	camx = mid(0,zoom*128-128,
 											p.x*zoom-(64-4*zoom))
 camy = mid(0,zoom*128-128,
 											p.y*zoom-(64-4*zoom))
 camera(camx,camy)

 --draw map w/ loop
 --so we can zoom
 for x=0,15 do for y=0,15 do 
 	sp = mget(x,y)
 	px=-flr(-x*8*zoom)
 	py=-flr(-y*8*zoom)
 	sw=-flr(-8*zoom)
 	-- sprite
 	if sp!=0 then
	 	sspr(sp*8, 0,  8,  8,
	 						px,   py, sw, sw)
	 end
	 -- sprite number and flag
	 if zoom > no_draw then
		 print(sp,
		 						px + sw/2,
		 						py + sw/2-1,
		 						0
		 						)
		 print(sp,
		 						px + sw/2-1,
		 						py + sw/2-2,
		 						fget(sp,0) and 8 or 12
		 						--12
		 						)
		 -- border
		 rect(px,py, px+sw,py+sw, 1)
		end
 end end

 -- draw character
 if p.x%8>2 and 
 			btn()!=0 and 
 			btn()<16 then 
 	sp = 32
 else 
 	sp = 24
 end
 -- sprite
 x = -flr(-p.x*zoom)
 y = -flr(-p.y*zoom)
 sspr(sp, 0, 8,  8,
 					x,  y, sw, sw, 
 					btn(0))

 -- outline
--[[ rect(x, y, x+sw, y+sw, 14)--]]
--[[
 -- collide corners
 c = tl and 8 or 5
 circfill(x,   y,   sw/8,c)
 c = tr and 8 or 5
 circfill(x+sw,y,   sw/8,c)
 c = bl and 8 or 5
 circfill(x,y+sw,   sw/8,c)
 c = br and 8 or 5
 circfill(x+sw,y+sw,sw/8,c)--]]

--[[ rect(-flr(-flr((p.x+4)/8)*8*zoom), 
 					-flr(-flr((p.y+4)/8)*8*zoom),
 					-flr(-flr((p.x+4)/8)*8*zoom)+sw,
 					-flr(-flr((p.y+4)/8)*8*zoom)+sw,7)--]]
--[[
 line(x,y-5,x,y-14,10)
 line(x,y-5,x-3,y-5-3,10)
 line(x,y-5,x+3,y-5-3,10)--]]
 if zoom > no_draw then
		for i, a in pairs({{-1,  -1},{sw+1,-1},
														       {-1,sw+1},{sw+1,sw+1}}) do
		 if collisions[i] then 
		 	circfill(x+a[1], y+a[2], 2, 8)
		 else
		  circfill(x+a[1], y+a[2], 2, 10)
		 end
		end
	end
end

