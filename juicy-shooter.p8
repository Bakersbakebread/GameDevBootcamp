pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
function _init()
 ship = {x=64,y=94, 
 								dx=0,dy=0,
 							 sp=1,fp=false,
 							 thrust=false,
 							 mxspd=5,slw=0,
 							 ts={{0,3,6},
 							 				{1,2,3},
 							 				{4,5,6}},
 							 frate=2,
 							 flash={},
 							 acc=.7,
 							 lastd=0,dashcd=.8}
	t = 0
	bullets = {}
end

function _update()
 t += 1
 ship.sp = 0
 ship.thrust = false
 ship.fp = false
	ship.dx *= .9
	ship.dy *= .9
	if btn(0) then
		ship.sp += 4
		ship.fp=true
		ship.dx = ((ship.slw-ship.mxspd)
													-ship.dx)/2
	 if ship.lastd == 0 then
	  ship.x -= 2
	  ship.lastd = -1
	 else
	 	ship.lastd *= dashcd
	 end
	elseif btn(1) then
	 ship.sp += 4
	 ship.dx = ((ship.mxspd-ship.slw)
	 											-ship.dx)/2
	 if ship.lastd == 0 then
	  ship.x += 2
	  ship.lastd = 1
	 else
	 	ship.lastd *= dashcd
	 end
	else
		ship.lastd *= dashcd
		if abs(ship.lastd) < .05 then
			ship.lastd = 0
		end
	end
	if btn(2) then
		ship.thrust=true
		ship.dy = ((ship.slw-ship.mxspd)
													-ship.dy)/2
	elseif btn(3) then
	 ship.dy = ((ship.mxspd-ship.slw)
	 											-ship.dy)/2
	end
	
		ship.sp += (t/2)%4 + 1
	
	ship.x += ship.dx
	ship.y += ship.dy
	
	if btn(4) and (t%ship.frate==0) then
		f = ship.frate
		lr = ((t/2)%f)/f*4+1
		if ship.fp then
		 gx = ship.ts[3][lr]
		elseif ship.sp > 4.5 then
			gx = ship.ts[2][lr]
		else
		 gx = ship.ts[1][lr]
		end
		gx += ship.x
		gy = ship.y+3
		acc = 5-ship.acc*5
		gdx = rnd(acc) - acc/2
		ship.dy -= .2
		ship.dx *= .9
		ship.y += 2
		add(bullets, shoot(gx,gy,gdx))
		add(ship.flash, {x=gx,y=gy-1})
	end
	
	update_shots()
end

function _draw()
 cls()
	spr(ship.sp,ship.x,ship.y,
					1,1,ship.fp)
	if ship.thrust then
		s=ship.ts[1]
		if ship.sp > 4.5 then
			s=ship.ts[2]
			if ship.fp then
				s=ship.ts[3]
			end
		end
	 line(ship.x+s[1],ship.y+6,
	 					ship.x+s[1],ship.y+6+rnd(1.2),
	 					rnd(1)+9)
	 line(ship.x+s[3],ship.y+6,
	 					ship.x+s[3],ship.y+6+rnd(1.2),
	 					rnd(1)+9)
	 line(ship.x+s[2],ship.y+6,
	 					ship.x+s[2],ship.y+6+rnd(5),
	 					rnd(1)+9)
	end
	draw_shots()
	for f in all(ship.flash) do
		circfill(f.x,f.y,1,7)
		pset(f.x+2,
							f.y+1,7)
		pset(f.x-2,
							f.y+1,7)
		del(ship.flash, f)
	end
end


function shoot(x,y,dx,dy,dmg)
	local b = {
		x=x,
		y=y,
		dx=dx or 0,
		dy=dy or -5,
		dmg=dmg or 1
	}
	return b
end

function update_shots()
	for b in all(bullets) do
	 b.x+=b.dx
	 b.y+=b.dy
	 if b.y < -10 then
	 	del(bullets,b)
	 end
	end
end

function draw_shots()
	for b in all(bullets) do
	 circ(b.x,b.y,3,15)
	 circ(b.x,b.y+3,1,15)
	 circfill(b.x,b.y,2,7)
	end
end
